{
    "name": "zslint",
    "displayName": "zslint",
    "description": "Syntax checking for ZS",
    "version": "0.0.3",
    "publisher": "BloodWorkXGaming",
    "repository": {
        "type": "git",
        "url": "https://github.com/CraftTweaker/ZsLint"
    },
    "icon": "assets/zslint_logo.png",
    "galleryBanner": {
        "color": "#D3D3D3"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Linters"
    ],
    "devDependencies": {
        "@types/node": "^12.6.5",
        "tslint": "^5.8.0",
        "typescript": "^3.5.3",
        "vscode": "^1.1.6"
    },
    "activationEvents": [
        "onLanguage:zenscript"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "yarn run compile && node ./node_modules/vscode/bin/test"
    },
    "contributes": {
        "languages": [
            {
                "id": "zenscript",
                "aliases": [
                    "ZenScript",
                    "zenscript"
                ],
                "extensions": [
                    ".zs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "zenscript",
                "scopeName": "source.zenscript",
                "path": "./syntaxes/zenscript.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "@types/ws": "^6.0.1",
        "npm": "^6.0.1",
        "ws": "^7.1.0"
    }
}